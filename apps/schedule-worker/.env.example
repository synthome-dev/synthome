DATABASE_URL=postgresql://user:password@localhost:5432/proom
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key

# TikTok OAuth credentials
TIKTOK_CLIENT_ID=your-tiktok-client-id
TIKTOK_CLIENT_SECRET=your-tiktok-client-secret
TIKTOK_REDIRECT_URI=https://your-domain.com/api/auth/tiktok/callback

# Optional timezone (default: UTC)
TZ=UTC

# API Key Encryption Secret (for encrypting/decrypting stored API keys)
# Generate with: node -e "console.log(require('crypto').randomBytes(32).toString('hex'))"
API_KEY_ENCRYPTION_SECRET=your-32-byte-hex-secret-here